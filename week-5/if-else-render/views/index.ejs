<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" />
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js"></script>
    <title>WEB340</title>
  </head>
  <body>
    <div class="container">
      <br />
      <br/><br/>
      <h2>Science Fiction Authors</h2>
      <br>
      <% for (var k = 0; k < names.length; k++) { %>
      <!-- if -->
      <% if (names[k] == "Frank Herbert") { %>
      <div class="alert alert-success" role="alert"><strong><%= names[k] %></strong></div>
      <% } %>
      <!-- // end if -->
      <!-- if -->
      <% if (names[k] == "George Orwell") { %>
      <div class="alert alert-secondary" role="alert"><strong><%= names[k] %></strong></div>
      <% } %>
      <!-- // end if -->
      <!-- if -->
      <% if (names[k] == "Isaac Asimov") { %>
      <div class="alert alert-secondary" role="alert"><strong><%= names[k] %></strong></div>
      <% } %>
      <!-- // end if -->
      <!-- if -->
      <% if (names[k] == "Arthur C. Clark") { %>
      <div class="alert alert-secondary" role="alert"><strong><%= names[k] %></strong></div>
      <% } %>
      <!-- // end if -->
      <% } %>
      <!-- end for loop -->
  </div>
  </body>
</html>